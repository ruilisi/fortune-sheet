{
  "package_file_location": "./package.json",
  "no_confirm": false,
  "skip_git_pull": false,
  "skip_git_push": false,
  "release_message": true,
  "remote": "origin",
  "pre_commit_commands": [
    "npm run clean",
    "npm run build",
    "npm run test"
  ],
  "post_commit_commands": [],
  "post_complete_commands": [
    "cd tmp && npm publish",
    "npm run clean"
  ],
  "files_to_commit": [
    "./dist/**/*"
  ]
}
